{
  "system_settings": {
    "max_concurrent_agents": 1,
    "agent_timeout": 30,
    "memory_limit": "256MB",
    "max_iterations": 10,
    "cache_size": 50
  },
  "agent_config": {
    "model": "gpt-3.5-turbo",
    "temperature": 0.7,
    "max_tokens": 1000,
    "response_format": "text"
  },
  "optimization": {
    "enable_caching": true,
    "batch_processing": false,
    "async_execution": true,
    "memory_monitoring": true
  },
  "resource_limits": {
    "cpu_cores": 1,
    "memory_mb": 256,
    "disk_space_mb": 100
  }
}